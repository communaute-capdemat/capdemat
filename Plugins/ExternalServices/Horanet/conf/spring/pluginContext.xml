<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

  <bean id="ws" class="com.zenexity.booker.ws.WSClient">
    <constructor-arg index="0" value="${plugins.externalservices.horanet.end_point3}" />
  </bean>

  <bean id="horanetExternalService"
        class="fr.capwebct.capdemat.plugins.externalservices.horanet.service.HoranetService"
        parent="externalProviderServiceAdapter">
    <property name="label" value="Horanet" />
    <property name="endPoint" value="${plugins.externalservices.horanet.end_point}" />
    <property name="endPoint2" value="${plugins.externalservices.horanet.end_point2}" />
    <property name="endPoint3" value="${plugins.externalservices.horanet.end_point3}" />
    <property name="login" value="${plugins.externalservices.horanet.login}" />
    <property name="password" value="${plugins.externalservices.horanet.password}" />
    <property name="userSearchService" ref="userSearchService" />
    <property name="externalHomeFolderService" ref="externalHomeFolderService" />
    <property name="localAuthorityRegistry" ref="localAuthorityRegistry" />
    <property name="requestTypeService" ref="requestTypeService" />
    <property name="requestSearchService" ref="requestSearchService" />
    <property name="ws" ref="ws" />
  </bean>

</beans>
