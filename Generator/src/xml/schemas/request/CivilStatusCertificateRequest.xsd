  <xs:complexType name="CscrInformationsJuridiqueType">
    <xs:sequence>
      <xs:element name="NomMandataireJuridique" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
