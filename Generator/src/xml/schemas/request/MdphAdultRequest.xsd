<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.cg95.fr/cvq/schema/mar" 
  xmlns:mar="http://www.cg95.fr/cvq/schema/mar"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:cvq="http://www.cg95.fr/cvq/schema/common"
  elementFormDefault="qualified" 
  attributeFormDefault="unqualified">
  
  <xs:import namespace="http://www.cg95.fr/cvq/schema/common" 
    schemaLocation="../common/CVQCommonElements.xsd" />
    
  <xs:element name="MdphAdultRequest">
    <xs:annotation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
    Formulaire de demande(s) auprès de la MDPH (Maison Départementale des Personnes Handicapées) ADULTE
      </xs:documentation>
      <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
    Formulaire de demande(s) auprès de la MDPH (ADULTE)
      </xs:documentation>
      <xs:appinfo>
        <model namespace="social" />
        <common>
          <namespace name="social" />
          <steps>
            <step index="0" name="subject">
                <widgets>
                    <subject />
                </widgets>
            </step>
            <step index="1" name="representantLegal" required="false">
                <conditions>
                    <condition name="representantLegal"/>
                </conditions>
            </step>
            <step index="2" name="organismePrestationFamiliale">
              <conditions>
                <condition name="otherPayingOrganism" />
              </conditions>
            </step>
            <step index="3" name="situationFamiliale">
                <conditions>
                    <condition name="situationMaritale" />
                </conditions>
            </step>
            <step index="4" name="situationProfessionnelle" required="false">
                <conditions>
                  <condition name="estSalarieVous"  />
                  <condition name="estStagiaireVous"  />
                  <condition name="estNonSalarieVous" />
                  <condition name="estDemandeurEmploiVous" />
                  <condition name="estRetraiteVous" />
                  <condition name="estBeneficiairePensionVous" />
                  <condition name="estAutreVous" />
                  <condition name="estSalarieConjoint"  />
                  <condition name="estStagiaireConjoint"  />
                  <condition name="estNonSalarieConjoint" />
                  <condition name="estDemandeurEmploiConjoint" />
                  <condition name="estRetraiteConjoint" />
                  <condition name="estBeneficiairePensionConjoint" />
                  <condition name="estAutreConjoint" />
                </conditions>
            </step>
            <step index="5" name="autreSituation">
                <conditions>
                    <condition name="estAutreCas" />
                    <condition name="estUnCas" />
                    <condition name="typeLogement" />
                </conditions>
            </step>
            <step index="6" name="projetDeVie">
                  <conditions>
                       <condition name="choixProjetDeVie" />
                  </conditions>
            </step>
            <step index="7" name="demandeParcoursScolaireEtFormation" required="false">
                  <conditions>
                        <condition name="preferencesEtablissementOuService" />
                  </conditions>
            </step>
            <step index="8" name="demandeCartes" required="false">
            </step>
            <step index="9" name="demandePrestationCompensation" required="false">
                <conditions>
                    <condition name="aideHumaine" />
                    <condition name="aideTechnique" />
                    <condition name="amenagementLogementDemenagement" />
                    <condition name="amenagementVehicule" />
                    <condition name="chargesSpecifiques" />
                    <condition name="aideAnimaliere" />
                </conditions>
            </step>
            <step index="10" name="affilitationAidant" required="false">
            </step>
            <step index="11" name="demandeAllocationEtComplementRessources" required="false">
                <conditions>
                        <condition name="precedentsRevenusRecus" />
                </conditions>
            </step>
            <step index="12" name="demandeProfessionnelle" required="false">
                <conditions>
                        <condition name="orientationReclassement" />
                </conditions>
            </step>
            <step index="13" name="demandeOrientationMedicoSociale" required="false">
                <conditions>
                    <condition name="preferenceEtablissementOuService" />
                </conditions>
            </step>
 
            <step index="14" ref="document" />
            <step index="15" ref="validation" />
          </steps>
        </common>
      </xs:appinfo>
    </xs:annotation>

    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="cvq:RequestType">
          <xs:sequence>
          
  <!--        STEP 0 : Subject          -->
           <xs:element name="FaxSujet" type="cvq:PhoneType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Fax
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Fax
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <pdf />
                  <bo column="1" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="Nationalite" type="mar:NationaliteAvecSuisseType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nationalité
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nationalité
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <pdf />
                  <bo column="1" />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="DateNaissanceSujet" type="xs:date">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Date de naissance
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Date de naissance
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="LieuNaissanceSujet" type="mar:MarBirthPlaceType">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Lieu de naissance
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Lieu de naissance
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
             <xs:element name="DateArriveeFrance" type="xs:date" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                   Si vous résidiez à l'étranger, précisez votre date d'arrivée en France
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Si vous résidiez à l'étranger, précisez votre date d'arrivée en France
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
            </xs:element>
            <xs:element name="NumeroSecuriteSocialeSujet" >
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  N° de sécurité sociale
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  N° de sécurité sociale
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                    <validation jsregexp="[0-9]{13}" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
                 <xs:restriction base="xs:string">
                   <xs:maxLength value="13" />
                   <xs:pattern value="[0-9]{13}" />
                 </xs:restriction>
               </xs:simpleType>
            </xs:element>
             <xs:element name="NomOrganismeSecuriteSociale">
              <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nom de l'organisme de sécurité sociale
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nom de l'organisme de sécurité sociale
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                    <validation jsregexp="^.{0,50}$" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:maxLength value="50" />
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="PrecedentDossierMdph" type="mar:MarPrecedentDossierMdphType" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Si vous possédez déjà un n° de dossier MDPH, précisez
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Si vous possédez déjà un n° de dossier MDPH, précisez
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="PrecisionSiDomicileAupresOrganisme" type="xs:string" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Si vous avez élu domicile auprès d'un organisme (association, centre d'hébergement ou d'action sociale), précisez lequel
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Si vous avez élu domicile auprès d'un organisme (association, centre d'hébergement ou d'action sociale), précisez lequel
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="subject" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
  <!--        STEP 1 : Représentant Légal        -->
          <xs:element name="RepresentantLegal" type="mar:RepresentantLegalType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Représentant légal
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Représentant légal
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="trigger" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
             <xs:element name="NomResponsableLegal" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nom de la personne ou organisme
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nom de la personne ou organisme
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="AdresseResponsableLegal" type="cvq:AddressType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Adresse de la personne ou de l'organisme
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Adresse de la personne ou de l'organisme
                </xs:documentation>
                <xs:appinfo>
                  <model namespace="users" isTiedToRequest="true" />
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
           
           <xs:element name="TelResponsableLegal" type="cvq:PhoneType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Téléphone
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Téléphone
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="FaxResponsableLegal" type="cvq:PhoneType" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Fax
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Fax
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="EmailResponsableLegal" type="cvq:EmailType" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Adresse électronique
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Adresse électronique
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="representantLegal" />
                    <conditions>
                           <condition name="representantLegal" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
        
  <!--               STEP 2 : Organisme Prestations Familiales          -->      
            <xs:element name="NumeroAllocataire" type="xs:string">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Précisez le numéro d'allocataire
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Précisez le numéro d'allocataire
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="organismePrestationFamiliale" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="OrganismePayeur" type="mar:OrganismePayeurType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Organisme payeur des prestations
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Organisme payeur des prestations
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="organismePrestationFamiliale" />
                    <conditions>
                        <condition name="otherPayingOrganism" type="trigger" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element> 
            
            <xs:element name="NomAllocataire" type="cvq:LastNameType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nom de l'allocataire
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nom de l'allocataire
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="organismePrestationFamiliale" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>       
            
            <xs:element name="PrenomAllocataire" type="cvq:FirstNameType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Prénom de l'allocataire
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Prénom de l'allocataire
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="organismePrestationFamiliale" />
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="NomOrganismePayeur" type="xs:string" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nom de l'organisme
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nom de l'organisme
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="organismePrestationFamiliale" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element> 
            
            <xs:element name="DomiciliationOrganismePayeur" type="cvq:AddressType" minOccurs="0">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Adresse de l'organisme
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Adresse de l'organisme
                </xs:documentation>
                <xs:appinfo>
                  <model namespace="users" isTiedToRequest="true" />
                  <common>
                    <step name="organismePrestationFamiliale" />
                    <conditions>
                        <condition name="otherPayingOrganism" type="filled" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>   
            
 <!--           STEP 3 : Situation Familiale   -->  
        <xs:element name="SituationMaritale" type="mar:SituationFamilialeType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Vous êtes
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Vous êtes
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="situationFamiliale" />
                    <conditions>
                        <condition name="situationMaritale" type="trigger" />
                    </conditions>
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="DateSituationFamiliale" type="xs:date">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Depuis le
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Depuis le
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="situationFamiliale" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
            
            <xs:element name="NombreEnfantsACharge">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Nombre d'enfants à charge
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Nombre d'enfants à charge
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="situationFamiliale" />
                    <validation jsregexp="[0-9]{1,2}$" />
                  </common>
                  <fo />
                  <bo column="1" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation> 
              <xs:simpleType>
                 <xs:restriction base="xs:string">
                   <xs:maxLength value="2" />
                   <xs:pattern value="[0-9]{2}" />
                 </xs:restriction>
               </xs:simpleType>              
            </xs:element>
            
            <xs:element name="IdentiteConjoint" type="mar:MarConjointIdentiteType">
            <xs:annotation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
                  Si vous êtes en couple, précisez l'identité de votre conjoint(e)
                </xs:documentation>
                <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
                  Si vous êtes en couple, précisez l'identité de votre conjoint(e)
                </xs:documentation>
                <xs:appinfo>
                  <common>
                    <step name="situationFamiliale" />
                    <conditions>
                        <condition name="situationMaritale" type="filled" />
                   </conditions>
                  </common>
                  <fo />
                  <bo column="2" />
                  <pdf />
                </xs:appinfo>
              </xs:annotation>               
            </xs:element>
        
      <!--        STEP 4 : Situation Professionnelle         -->   
      <xs:element name="SituationProfessionnelleVous" type="mar:ChoixSituationProfessionnelleType">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Vous
          </xs:documentation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/shortdesc">
            Vous
          </xs:documentation>
          <xs:appinfo>
            <common>
              <step name="situationProfessionnelle" />
               <conditions>
                  <condition name="estSalarieVous" type="trigger" />
                  <condition name="estStagiaireVous" type="trigger" />
                  <condition name="estNonSalarieVous" type="trigger" />
                  <condition name="estDemandeurEmploiVous" type="trigger" />
                  <condition name="estRetraiteVous" type="trigger" />
                  <condition name="estBeneficiairePensionVous" type="trigger" />
                  <condition name="estAutreVous" type="trigger" />
               </conditions>
            </common>
            <fo />
            <bo column="1" />
            <pdf />
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      
      <xs:element name="SituationProfessionnelleVousSalarie" type="mar:MarSituationProfessionnelleSalarieType">
        <xs:annotation>
          <xs:documentation xml:lang="fr" source="http://www.cg95.fr/cvq/schema/longdesc">
            Salarié(e) y compris apprentissage
          </xs:documentation>
